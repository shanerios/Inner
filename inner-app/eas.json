{
  "cli": { "version": ">= 12.0.0", "appVersionSource":"remote" },
  "build": {
    "apk": {
      "channel": "preview",
      "android": { "buildType": "apk" }
    },
    "internal": {
      "channel": "internal",
      "android": { "buildType": "app-bundle" },
      "autoIncrement": true,
      "env": { "EXPO_NEW_ARCH_ENABLED": "0" }
    },
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": { "buildType": "apk" },
      "env": { "EXPO_NEW_ARCH_ENABLED": "0" }
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "app-bundle"
      },
      "autoIncrement": true,
      "env": { "EXPO_NEW_ARCH_ENABLED": "0" }
    }
  },
  "submit": {
    "internal": {
      "android": {
        "track": "internal"
      }
    }
  }
}