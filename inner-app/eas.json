        "simulator": false
      },
      "autoIncrement": true,
      "env": {
        "EXPO_NEW_ARCH_ENABLED": "0"
      }
    }
  },
  "submit": {
    "internal": {
      "ios": {
        "ascAppId": "6755213270"
      },
