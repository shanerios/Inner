    "preview": {
      "distribution": "store",
      "channel": "preview",
      "android": { "buildType": "app-bundle" },
      "ios": {
        "simulator": false
      },
      "autoIncrement": true,
      "env": {
        "EXPO_NEW_ARCH_ENABLED": "0"
      }
    },
    "production": {
      "distribution": "store",
