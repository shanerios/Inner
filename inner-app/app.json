{
  "expo": {
    "name": "Inner",
    "slug": "inner",
    "version": "1.1.5",
    "runtimeVersion": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": false,
    "splash": {
      "image": "./assets/splash_ios.png",
      "resizeMode": "contain",
      "backgroundColor": "#1f233a"
    },
    "ios": {
      "supportsTablet": true,
      "requireFullScreen": true,
      "bundleIdentifier": "com.getinner.app",
      "buildNumber": "2021",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "permissions": [
        "FOREGROUND_SERVICE_MEDIA_PLAYBACK",
        "com.android.vending.BILLING"
      ],
      "package": "com.getinner.app",
      "versionCode": 31,
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#1f233a"
      },
      "edgeToEdgeEnabled": true,
      "newArchEnabled": false,
      "notification": {
        "icon": "./assets/images/inner_orb_icon.png",
        "color": "#FFFFFF"
      }
    },
    "plugins": [
      [
        "./plugins/withTrackPlayerNotificationIcon",
        {
          "icon": "./assets/images/inner_orb_icon.png"
        }
      ],
      "expo-font",
      "expo-asset"
    ],
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "73977c48-2e46-4763-9e5d-15e0a6f25a48"
      }
    },
    "owner": "shanerios",
    "updates": {
      "url": "https://u.expo.dev/73977c48-2e46-4763-9e5d-15e0a6f25a48"
    }
  }
}
